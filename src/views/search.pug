extends base
include mixins/mixins

block contents 
    form(method="GET")
        input(name="keyword" type="text" placeholder="Input to Search")
        input(type="submit" value="Search")
        input(type="button" value="CANCEL" onclick="location.href=`/`")
    div
        each video in videofind
            +videoList(video)
        else 
            p Search Video